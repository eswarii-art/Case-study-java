class CompareArrays {
    public static void main(String[] args) {
        int[] a = {1, 2, 3};
        int[] b = {1, 2, 3};
        boolean same = true;
        if (a.length != b.length) {
            same = false;
        } else {
            for (int i = 0; i < a.length; i++) {
                if (a[i] != b[i]) {
                    same = false;
                    break;
                }
            }
        }

        if (same)
            System.out.println("Arrays are identical");
        else
            System.out.println("Arrays are not identical");
    }
}
