class BankAccount {
    private int balance = 1000;
    public int getBalance() {
        return balance;
    }
    public void setBalance(int b) {
        balance = b;
    }
    public static void main(String[] args) {
        BankAccount acc = new BankAccount();
        acc.setBalance(5000);
        System.out.println("Balance: " + acc.getBalance());
    }
}
